let buzz = document.createElement("button");
buzz.innerHTML = "Buzz";
buzz.addEventListener("click", function () {
  alert("Opening buzz in a new tab");
  window.open("https://iteea-ga-dekalb-dhhs.agilixbuzz.com/");
});
document.body.appendChild(buzz);

